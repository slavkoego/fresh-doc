from selenium import webdriver
import time

link = "http://www.motka-freshdoc.qds.ru"
try:
    browser = webdriver.Chrome()
    browser.get(link)

    input1 = browser.find_element_by_css_selector("div.header--main__wrap .qd__navigation__item .button-login")
    input1.click()
    time.sleep(2)
    input2 = browser.find_element_by_css_selector(".qd-overlay-auth__input:nth-child(1)")
    input2.send_keys("test.qds.777@bk.ru")
    input3 = browser.find_element_by_css_selector(".qd-overlay-auth__input:nth-child(2)")
    input3.send_keys("AnSa1991")
    option1 = browser.find_element_by_css_selector(".qd-overlay-auth__form .qd-overlay-auth__button")
    option1.click()

finally:
    time.sleep(30)
    browser.quit()